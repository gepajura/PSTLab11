<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".webSite">

    <RelativeLayout
        android:id="@+id/relativeLayout3"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/volver"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/volver"
            android:layout_marginHorizontal="@dimen/margenes"
            android:layout_marginTop="@dimen/marginTop"
            android:onClick="btnVolver"/>

        <WebView
            android:id="@+id/webSite"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/volver"
            android:layout_marginHorizontal="@dimen/marginWeb"
            android:layout_marginVertical="@dimen/marginWeb"/>

    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>